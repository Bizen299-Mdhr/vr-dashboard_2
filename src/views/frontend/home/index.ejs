                                            <h3><span></span></h3>
                                            <input hidden id="home-remarks" value="<%= homeInfo?.remarks %>">
                                        </div>
                                    </div>
                                </div>

                            </div>

                        </div>
                    </section>
                    <!-- =========    HOME PAGE END    ======== -->
                    <%- include('../aboutme/about-me')  %>
                    <%- include('../resume/resume')  %>
                    <%- include('../portfolio/portfolio')  %>
