                <div class="slide-nav-btn">
                    <span></span>
                </div>
                <!-- =========    SIDEBAR START    ======== -->
                <div class="sidebar">
                    <nav>
                        <!-- MENU -->
                        <ul id="navbar" role="tablist">
                            <li class="active">
                                <a href="index.html#home" aria-controls="home" role="tab" data-toggle="tab">
                                    <i class="fa fa-home"></i>
                                </a>
                            </li>
                            <li>
                                <a href="index.html#about" aria-controls="about" role="tab" data-toggle="tab">
                                    <i class="fa fa-address-card-o"></i>
                                </a>
                            </li>
                            <li>
                                <a href="index.html#resume" aria-controls="resume" role="tab" data-toggle="tab">
                                    <i class="fa fa-book"></i>
                                </a>
                            </li>
                            <li>
                                <a href="index.html#portfolio" aria-controls="portfolio" role="tab" data-toggle="tab">
                                    <i class="fa fa-desktop"></i>
                                </a>
                            </li>
                            <li>
                                <a href="index.html#blog" aria-controls="blog" role="tab" data-toggle="tab">
                                    <i class="fa fa-newspaper-o"></i>
                                </a>
                            </li>
                            <li>
                                <a href="index.html#contact" aria-controls="contact" role="tab" data-toggle="tab">
                                    <i class="fa fa-comments-o"></i>
                                </a>
                            </li>
                        </ul>

                        <!-- SOCIAL ICONS -->
                        <div class="social text-center">
                            <a href="<%=homeInfo?.facebook_url%>" target="_blank">
                                <i class="fa fa-facebook"></i>
                            </a>

                            <a href="<%=homeInfo?.twitter_url%>" target="_blank">
                                <i class="fa fa-twitter"></i>
                            </a>

                            <a href="<%=homeInfo?.linkedin_url%>" target="_blank">
                                <i class="fa fa-linkedin"></i>
                            </a>

                        </div>
