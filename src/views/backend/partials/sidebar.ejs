<div class="iq-sidebar">
   <div class="iq-sidebar-logo d-flex justify-content-between">
      <a href="index.html" class="header-logo">
         <img src="/backend/assets/img/cv-resume-portfolio.webp" class="img-fluid rounded-normal" alt="">
         <div class="logo-title">
            <span class="text-primary text-uppercase"><%=cmsTitle%></span>
         </div>
      </a>
      <div class="iq-menu-bt-sidebar">
         <div class="iq-menu-bt align-self-center">
            <div class="wrapper-menu">
               <div class="main-circle"><i class="las la-bars"></i></div>
            </div>
         </div>
      </div>
   </div>
   <div id="sidebar-scrollbar">
      <nav class="iq-sidebar-menu">
         <ul id="iq-sidebar-toggle" class="iq-menu">
            <% let checkUrl=url; let match_url=checkUrl.match('^[^?]*')[0];
               modulePermissions.forEach(function(modulePermission) { let collapsed='collapsed' ; let collapsed_in='' ;
               for (let subPage in modulePermission.subPages) { if (match_url=='/' +subPage) { collapsed='' ;
               collapsed_in='in' ; break; } } %>
               <%if (modulePermission.enableDropdown) { %>
                  <li  data-match = "<%- match_url  %>" class=" <% if(modulePermission.subPages[match_url.replace('/','')]) { %> active <% }%>">
                     <a href="#<%= modulePermission?.id %>" class="iq-waves-effect" data-toggle="collapse" aria-expanded="<% if(typeof (previousUrl) !== 'undefined' && match_url !== '/'+modulePermission.id) { %>false <% }else{ %> true <% }%>"><span
                           class="ripple rippleEffect"></span><i class="las la-home iq-arrow-left"></i><span>
                           <%= modulePermission?.title %>
                        </span><i class="ri-arrow-right-s-line iq-arrow-right"></i></a>
                     <ul  <% if(match_url == '/'+modulePermission.id || collapsed_in=='in') {%> class="iq-submenu collapse show"<% } else { %> class="iq-submenu collapse " <% } %> id="<%= modulePermission.id %>" data-parent="#iq-sidebar-toggle">
                        <% for (var subPage in modulePermission.subPages) { %>
                            <li class="nav-sub-item <% if(match_url == '/'+subPage) { %> active active-menu <% } %>">
                                <a class="nav-sub-link text-nowrap" href="/system/<%=  subPage %>">
                                       <%= modulePermission?.subPages?.[subPage] %>
                                </a>
                            </li>
                        <% }%>
                    </ul>
                  </li>

               </li>
                  <% }else { %>
                     <li
                        class="iq-waves-effect <% if(match_url == '/'+modulePermission.id ) { %> active <% } else { %> remove-active <% } %>">
                        <a href="/system/<%= modulePermission.id %>" class="nav-link">
                           <i class=" <%= modulePermission.icon %>"></i>
                           <span class="sidemenu-label text-nowrap">
                              <%= modulePermission?.title %>
                           </span>
                        </a>
                     </li>
                     <% } }); %>
         </ul>
      </nav>
   </div>


</div>